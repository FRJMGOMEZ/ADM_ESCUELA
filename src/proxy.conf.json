{
    "/api": {
        "target": "http://localhost:3000",
        "secure": false,
        "logLevel": "debug",
        "changeOrigin": true,
        "ws": true,
        "autoRewrite": true
    },

    "/sock/*": {
        "target": "http://localhost:3000/socket.io/",
        "ws": true,
        "logLevel": "debug"
    }
}